{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "jewel_parser",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "expression"
          },
          {
            "type": "SYMBOL",
            "name": "_statement"
          },
          {
            "type": "SYMBOL",
            "name": "type_declaration"
          }
        ]
      }
    },
    "_literal": {
      "type": "PREC_RIGHT",
      "value": 1,
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_number"
          },
          {
            "type": "SYMBOL",
            "name": "string"
          },
          {
            "type": "SYMBOL",
            "name": "array_literal"
          },
          {
            "type": "SYMBOL",
            "name": "struct_literal"
          }
        ]
      }
    },
    "string": {
      "type": "PATTERN",
      "value": "\"[^\"]*\""
    },
    "array_literal": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "expression"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ","
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "struct_literal": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "struct_name",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "identifier"
            },
            "named": true,
            "value": "struct_name"
          }
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "SYMBOL",
          "name": "_struct_field"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "_struct_field"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ","
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "_struct_field": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "field_name",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "identifier"
            },
            "named": true,
            "value": "field_name"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "field_value",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "expression"
            },
            "named": true,
            "value": "field_value"
          }
        }
      ]
    },
    "_number": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "integer"
        },
        {
          "type": "SYMBOL",
          "name": "float"
        }
      ]
    },
    "_digit_sequence": {
      "type": "PATTERN",
      "value": "[0-9_]+"
    },
    "integer": {
      "type": "SYMBOL",
      "name": "_digit_sequence"
    },
    "float": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_digit_sequence"
        },
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "SYMBOL",
          "name": "_digit_sequence"
        }
      ]
    },
    "expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_math_expression"
        },
        {
          "type": "SYMBOL",
          "name": "_literal"
        }
      ]
    },
    "_math_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_number"
        },
        {
          "type": "SYMBOL",
          "name": "_arithmetic_operator"
        },
        {
          "type": "SYMBOL",
          "name": "negation"
        }
      ]
    },
    "_arithmetic_operator": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "addition"
        },
        {
          "type": "SYMBOL",
          "name": "subtraction"
        },
        {
          "type": "SYMBOL",
          "name": "multiplication"
        },
        {
          "type": "SYMBOL",
          "name": "division"
        }
      ]
    },
    "addition": {
      "type": "PREC_LEFT",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_math_expression"
          },
          {
            "type": "STRING",
            "value": "+"
          },
          {
            "type": "SYMBOL",
            "name": "_math_expression"
          }
        ]
      }
    },
    "subtraction": {
      "type": "PREC_LEFT",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_math_expression"
          },
          {
            "type": "STRING",
            "value": "-"
          },
          {
            "type": "SYMBOL",
            "name": "_math_expression"
          }
        ]
      }
    },
    "multiplication": {
      "type": "PREC_LEFT",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_math_expression"
          },
          {
            "type": "STRING",
            "value": "*"
          },
          {
            "type": "SYMBOL",
            "name": "_math_expression"
          }
        ]
      }
    },
    "division": {
      "type": "PREC_LEFT",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_math_expression"
          },
          {
            "type": "STRING",
            "value": "/"
          },
          {
            "type": "SYMBOL",
            "name": "_math_expression"
          }
        ]
      }
    },
    "negation": {
      "type": "PREC_RIGHT",
      "value": 3,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "-"
          },
          {
            "type": "SYMBOL",
            "name": "_math_expression"
          }
        ]
      }
    },
    "type_notation": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "type"
        }
      ]
    },
    "type_declaration": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "struct_type"
        },
        {
          "type": "SYMBOL",
          "name": "data_type"
        }
      ]
    },
    "type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_primitive_type"
        },
        {
          "type": "SYMBOL",
          "name": "user_defined_type"
        },
        {
          "type": "SYMBOL",
          "name": "array_type"
        }
      ]
    },
    "user_defined_type": {
      "type": "PATTERN",
      "value": "[A-Z][a-zA-Z0-9]*"
    },
    "_primitive_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "integer_type"
        },
        {
          "type": "SYMBOL",
          "name": "float_type"
        },
        {
          "type": "SYMBOL",
          "name": "string_type"
        },
        {
          "type": "SYMBOL",
          "name": "boolean_type"
        }
      ]
    },
    "integer_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "Int"
        },
        {
          "type": "STRING",
          "value": "Int32"
        },
        {
          "type": "STRING",
          "value": "Int64"
        }
      ]
    },
    "float_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "Float"
        },
        {
          "type": "STRING",
          "value": "Float32"
        },
        {
          "type": "STRING",
          "value": "Float64"
        }
      ]
    },
    "string_type": {
      "type": "STRING",
      "value": "String"
    },
    "boolean_type": {
      "type": "STRING",
      "value": "Bool"
    },
    "array_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "[]"
        },
        {
          "type": "SYMBOL",
          "name": "type"
        }
      ]
    },
    "data_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "data"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "user_defined_type"
          },
          "named": true,
          "value": "data_type_name"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "type"
          },
          "named": true,
          "value": "data_type_type"
        }
      ]
    },
    "struct_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "struct"
        },
        {
          "type": "FIELD",
          "name": "struct_name",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "user_defined_type"
            },
            "named": true,
            "value": "struct_name"
          }
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "SYMBOL",
          "name": "_struct_member"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "_struct_member"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ","
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "_struct_member": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "field_name",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "identifier"
            },
            "named": true,
            "value": "field_name"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "field_type",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "type"
            },
            "named": true,
            "value": "field_type"
          }
        }
      ]
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "let_assignment"
        },
        {
          "type": "SYMBOL",
          "name": "return_statement"
        }
      ]
    },
    "let_assignment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "let"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          },
          "named": true,
          "value": "let_identifier"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "type_notation"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "return_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "return"
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [
    "expression",
    "type",
    "type_declaration"
  ],
  "reserved": {}
}