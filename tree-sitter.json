{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "jewel_parser",
      "camelcase": "JewelParser",
      "title": "The Jewel Programming Language Parser",
      "scope": "source.jewel_parser",
      "file-types": [
        ".jwl"
      ],
      "injection-regex": "^jewel_parser$",
      "class-name": "TreeSitterJewelParser"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A tree-sitter parser for the Jewel Programming Language",
    "authors": [
      {
        "name": "Avram Eisner",
        "email": "avrame@gmail.com",
        "url": "https://www.avrameisner.com/"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-jewel_parser"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}